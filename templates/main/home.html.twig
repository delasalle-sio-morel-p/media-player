{% extends 'layout.html.twig' %}

{% block button %}
    <button class="btn btn-outline-success my-2 my-sm-0" type="submit">
        {% if status == false %}
            <a href="{{ path("login")}}">Log in</a>
        {% else %}
            <a href="{{ path("logout")}}">Log out</a>
        {% endif %}
    </button>
{% endblock %}

